apiVersion: v1
kind: Config
clusters:
- name: kubernetes
  cluster:
    certificate-authority: /etc/kubernetes/ssl/k8s-ca.pem
    server: https://vogt1005.scripps.edu:6443
users:
- name: system:node:XXX
  user:
    client-certificate: /etc/kubernetes/ssl/peer.pem
    client-key: /etc/kubernetes/ssl/peer-key.pem
contexts:
- context:
    cluster: kubernetes
    user: system:node:XXX
  name: kubernetes
current-context: kubernetes
