apiVersion: v1
kind: Config
clusters:
- name: kubernetes
  cluster:
    certificate-authority: /etc/kubernetes/ssl/k8s-ca.pem
    server: https://vogt1005.scripps.edu:6443
users:
- name: system:kube-proxy
  user:
    client-certificate: /etc/kubernetes/ssl/proxy.pem
    client-key: /etc/kubernetes/ssl/proxy-key.pem
contexts:
- context:
    cluster: kubernetes
    user: system:kube-proxy
  name: default
current-context: default
